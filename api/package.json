{
  "name": "api",
  "scripts": {
    "copy-assets": "ts-node copyStaticAssets.ts",
    "build": "tsc && npm run copyAssets",
    "lint": "tsc --noEmit",
    "start": "npm run build && npm run serve",
    "serve": "node dist/src/index.js",
    "watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "dev": "npm run copy-assets && concurrently \"npm run watch\" \"npm run serve-debug\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "@types/shelljs": "^0.8.8",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.5",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/maxmind": "^2.0.5",
    "cors": "^2.8.5",
    "maxmind": "^4.3.0"
  }
}
