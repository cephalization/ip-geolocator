{
  "name": "api",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "start": "npm run build && npm run serve",
    "serve": "node dist/src/index.js",
    "watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run serve-debug\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.5",
    "typescript": "^4.0.3"
  }
}
